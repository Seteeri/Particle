(de mov-*1 (Line)
  # Move to bnds of current line
  (let (Ori (get Line 'ori)
        Dims (get Line 'line 'dims))
    (set-cur-x (+ (car Ori)
                  (car Dims)))
    (set-cur-y (- (cadr Ori)
                  (cadr Dims)))
    (nl-cur)
    (println 'mov-*1 "Moved *1 to: " (format (get *cur 1) *Scl) (format (get *cur 2) *Scl))
    (mov> *1 *cur)))
